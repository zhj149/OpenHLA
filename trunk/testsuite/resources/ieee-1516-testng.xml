<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="OHLA IEEE 1516 Testsuite" parallel="false" verbose="5">
  <test name="IEEE 1516 - Federation Management">
    <classes>
      <class name="net.sf.ohla.rti.testsuite.hla.rti1516.federation.CreationTestNG"/>
      <class name="net.sf.ohla.rti.testsuite.hla.rti1516.federation.JoiningTestNG"/>
      <class name="net.sf.ohla.rti.testsuite.hla.rti1516.federation.SynchronizationTestNG"/>
    </classes>
  </test>
  <test name="IEEE 1516 - Declaration Management">
    <classes>
      <class name="net.sf.ohla.rti.testsuite.hla.rti1516.declaration.ObjectDeclarationTestNG"/>
      <class name="net.sf.ohla.rti.testsuite.hla.rti1516.declaration.InteractionDeclarationTestNG"/>
    </classes>
  </test>
  <test name="IEEE 1516 - Object Management">
    <classes>
      <class name="net.sf.ohla.rti.testsuite.hla.rti1516.object.ObjectNameReservationTestNG"/>
      <class name="net.sf.ohla.rti.testsuite.hla.rti1516.object.ObjectRegistrationTestNG"/>
      <class name="net.sf.ohla.rti.testsuite.hla.rti1516.object.ObjectUpdateTestNG"/>
      <class name="net.sf.ohla.rti.testsuite.hla.rti1516.object.ObjectDeletionTestNG"/>
      <class name="net.sf.ohla.rti.testsuite.hla.rti1516.object.InteractionTestNG"/>
    </classes>
  </test>
  <test name="IEEE 1516 - Ownership Management">
    <classes>
      <class name="net.sf.ohla.rti.testsuite.hla.rti1516.ownership.AttributeOwnershipTestNG"/>
      <class name="net.sf.ohla.rti.testsuite.hla.rti1516.ownership.AttributeDivestitureTestNG"/>
      <class name="net.sf.ohla.rti.testsuite.hla.rti1516.ownership.AttributeAcquisitionTestNG"/>
      <class name="net.sf.ohla.rti.testsuite.hla.rti1516.ownership.NegotiatedAttributeDivestitureTestNG"/>
      <class name="net.sf.ohla.rti.testsuite.hla.rti1516.ownership.IntrusiveAttributeAcquisitionTestNG"/>
    </classes>
  </test>
  <test name="IEEE 1516 - Data Distribution Management">
    <classes>
      <class name="net.sf.ohla.rti.testsuite.hla.rti1516.datadistribution.RegionSupportTestNG"/>
      <class name="net.sf.ohla.rti.testsuite.hla.rti1516.datadistribution.ObjectRegionTestNG"/>
      <class name="net.sf.ohla.rti.testsuite.hla.rti1516.datadistribution.InteractionRegionTestNG"/>
    </classes>
  </test>
  <test name="IEEE 1516 - Support Services">
    <classes>
      <class name="net.sf.ohla.rti.testsuite.hla.rti1516.support.FederateSupportTestNG"/>
      <class name="net.sf.ohla.rti.testsuite.hla.rti1516.support.OrderSupportTestNG"/>
      <class name="net.sf.ohla.rti.testsuite.hla.rti1516.support.TransportationSupportTestNG"/>
      <class name="net.sf.ohla.rti.testsuite.hla.rti1516.support.ObjectSupportTestNG"/>
      <class name="net.sf.ohla.rti.testsuite.hla.rti1516.support.InteractionSupportTestNG"/>
      <class name="net.sf.ohla.rti.testsuite.hla.rti1516.support.DimensionSupportTestNG"/>
    </classes>
  </test>
  <test name="IEEE 1516 - Time Management">
    <classes>
      <class name="net.sf.ohla.rti.testsuite.hla.rti1516.time.TimeAdvancingExceptionsTestNG"/>
      <class name="net.sf.ohla.rti.testsuite.hla.rti1516.time.TimeConstrainedTestNG"/>
      <class name="net.sf.ohla.rti.testsuite.hla.rti1516.time.EnableTimeConstrainedExceptionsTestNG"/>
      <class name="net.sf.ohla.rti.testsuite.hla.rti1516.time.TimeRegulationTestNG"/>
      <class name="net.sf.ohla.rti.testsuite.hla.rti1516.time.EnableTimeRegulationExceptionsTestNG"/>
      <class name="net.sf.ohla.rti.testsuite.hla.rti1516.time.TimeAdvanceRequestTestNG"/>
      <class name="net.sf.ohla.rti.testsuite.hla.rti1516.time.TimeAdvanceRequestAvailableTestNG"/>
      <class name="net.sf.ohla.rti.testsuite.hla.rti1516.time.NextMessageRequestTestNG"/>
      <class name="net.sf.ohla.rti.testsuite.hla.rti1516.time.NextMessageRequestAvailableTestNG"/>
    </classes>
  </test>
</suite>
