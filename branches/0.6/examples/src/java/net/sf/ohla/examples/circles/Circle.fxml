<?import javafx.scene.Group?>
<?import javafx.scene.paint.Color?>
<?import javafx.scene.shape.Circle?>

<Group xmlns:fx="http://javafx.com/javafx/2.2"
        fx:id="group"
        fx:controller="net.sf.ohla.examples.circles.CircleController">
  <Circle fx:id="radiusRing"
          strokeWidth="5">
    <fill>
      <Color opacity="0"/>
    </fill>
    <stroke>
      <Color/>
    </stroke>
  </Circle>
  <Circle fx:id="background"/>
</Group>
